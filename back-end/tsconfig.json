{
  "compilerOptions": {
      "lib": ["es2016", "dom"],
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "typeRoots": ["./types", "./node_modules/@types"],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["./types/**/*.ts", "./**/*.ts"],
    "exclude": ["node_modules"]
}
